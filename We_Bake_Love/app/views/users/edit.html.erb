<%= stylesheet_link_tag "user_edit.css" %>
<div class="editform">
<%= form_for @user do |f| %>

<%= fa_icon "user solid 3x", class:"user_icon" %>
<h2>Edit_Profile</h2>

<div class="space">
  <%= f.label :name ,:class=>"name_space"%>
  <%= f.text_field :name %>
</div>

<div class="space">
  <%= f.label :change_password ,:class=>"password_space"%>
  <%= f.password_field :password %>
</div>

<div class="new_user_button">
  <%= f.submit %>
</div>
<% end %>
</div>